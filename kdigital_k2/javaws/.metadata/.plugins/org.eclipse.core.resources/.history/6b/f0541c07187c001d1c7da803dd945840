package hello;

import java.io.BufferedWriter;
import java.util.Scanner;

public class Hello {

	public static void main(String[] args) {
		//4-12
		for(int i=1; i<4; i++) {
			for(int j=2; j<5 ; j++ ) {
				System.out.print(i+"*"+j+"="+ i*j);
				System.out.print("     ");
			}
			System.out.println();
		}
			
		
	
	}



}

